@using Microsoft.AspNetCore.Authentication

<h2>Claims</h2>
<div>
    <a href="@Url.Action("Logout", "Home")">LogOut</a><br />
    <a href="@Url.Action("GetResource", "Home")">GetResource</a>
</div>
<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>